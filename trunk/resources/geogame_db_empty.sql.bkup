/*Table structure for table `road_movement` */

DROP TABLE IF EXISTS `road_movement`;

CREATE TABLE `road_movement` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `road_id` bigint(20) NOT NULL,
  `forward` bit(1) NOT NULL,
  `move_start` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `duration` int(11) NOT NULL,
  `status` int(11) DEFAULT NULL,
  `user_id` bigint(20) NOT NULL,
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=91817 DEFAULT CHARSET=latin1;

/*Data for the table `road_movement` */

/*Table structure for table `trade_offer` */

DROP TABLE IF EXISTS `trade_offer`;

CREATE TABLE `trade_offer` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `player1_id` bigint(20) NOT NULL,
  `player2_id` bigint(20) NOT NULL,
  `player1_money` int(11) NOT NULL,
  `player2_money` int(11) NOT NULL,
  `player1_items` varchar(500) NOT NULL,
  `player2_items` varchar(500) NOT NULL,
  `status` int(11) NOT NULL,
  `time_created` datetime NOT NULL,
  `time_replied` datetime DEFAULT NULL,
  `delivered` tinyint(11) NOT NULL DEFAULT '0',
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `players_ids_idx` (`player1_id`,`player2_id`),
  KEY `delivered` (`delivered`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Data for the table `trade_offer` */

/*Table structure for table `transaction_log` */

DROP TABLE IF EXISTS `transaction_log`;

CREATE TABLE `transaction_log` (
  `method_name` varchar(50) DEFAULT NULL,
  `username` varchar(50) DEFAULT NULL,
  `start_time` bigint(20) DEFAULT NULL,
  `end_time` bigint(20) DEFAULT NULL,
  `duration` int(11) DEFAULT NULL,
  KEY `method_name_idx` (`method_name`),
  KEY `username_idx` (`username`),
  KEY `duration_idx` (`duration`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

/*Data for the table `transaction_log` */

/*Table structure for table `user` */

DROP TABLE IF EXISTS `user`;

CREATE TABLE `user` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `username` varchar(50) DEFAULT NULL,
  `password` varchar(50) DEFAULT NULL,
  `first_name` varchar(50) DEFAULT NULL,
  `last_name` varchar(50) DEFAULT NULL,
  `admin` tinyint(1) DEFAULT NULL,
  `score` int(11) DEFAULT NULL,
  `latitude` double DEFAULT NULL,
  `longitude` double DEFAULT NULL,
  `icon_filename` varchar(20) DEFAULT NULL,
  `status_id` int(11) DEFAULT NULL,
  `auth_code` varchar(32) DEFAULT NULL,
  `current_location_id` bigint(20) DEFAULT NULL,
  `current_road_id` bigint(20) DEFAULT NULL,
  `forward` tinyint(1) DEFAULT NULL,
  `logged_in` tinyint(1) NOT NULL,
  `is_moving` tinyint(1) NOT NULL,
  `current_road_movement_id` bigint(20) DEFAULT NULL,
  `last_request` datetime NOT NULL,
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `idx_username` (`username`)
) ENGINE=InnoDB AUTO_INCREMENT=369 DEFAULT CHARSET=latin1;



/*Table structure for table `user_graph` */

DROP TABLE IF EXISTS `user_graph`;

CREATE TABLE `user_graph` (
  `user1_id` bigint(20) NOT NULL DEFAULT '0',
  `user2_id` bigint(20) NOT NULL DEFAULT '0',
  PRIMARY KEY (`user1_id`,`user2_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

