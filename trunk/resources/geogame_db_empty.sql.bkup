/*Table structure for table `message` */

DROP TABLE IF EXISTS `message`;

CREATE TABLE `message` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `sender_id` bigint(20) DEFAULT NULL,
  `time_sent` datetime DEFAULT NULL,
  `broadcast` tinyint(1) DEFAULT NULL,
  `global` tinyint(1) DEFAULT NULL,
  `content` varchar(1000) DEFAULT NULL,
  `delivered` tinyint(11) NOT NULL DEFAULT '0',
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `delivered_idx` (`delivered`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Data for the table `message` */

/*Table structure for table `message_graph` */

DROP TABLE IF EXISTS `message_graph`;

CREATE TABLE `message_graph` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `message_id` bigint(20) NOT NULL,
  `receiver_id` bigint(20) NOT NULL,
  `time_received` datetime DEFAULT NULL,
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`,`message_id`,`receiver_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

/*Data for the table `message_graph` */

/*Table structure for table `road` */

DROP TABLE IF EXISTS `road`;

CREATE TABLE `road` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) DEFAULT NULL,
  `location1_id` bigint(20) DEFAULT NULL,
  `location2_id` bigint(20) DEFAULT NULL,
  `road_type` int(11) DEFAULT NULL,
  `road_points_string` varchar(1000) DEFAULT NULL,
  `code` varchar(40) DEFAULT NULL,
  `danger` double NOT NULL DEFAULT '0',
  `length` double NOT NULL DEFAULT '0',
  `travel_duration` int(11) NOT NULL DEFAULT '0',
  `penalty` tinyint(1) NOT NULL,
  `created` datetime NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `code_idx` (`code`)
) ENGINE=InnoDB AUTO_INCREMENT=143 DEFAULT CHARSET=latin1;

